package com.cts.cart.entity;

public class Cart {

	private int cartId;
	private String name;
	private Product item;
	private int qty;

	public int getCartId() {
		return cartId;
	}

	public void setCartId(int cartId) {
		this.cartId = cartId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Product getItem() {
		return item;
	}

	public void setItem(Product item) {
		this.item = item;
	}

	public int getQty() {
		return qty;
	}

	public void setQty(int qty) {
		this.qty = qty;
	}

	public Cart() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Cart(String name, Product item, int qty) {
		super();
		this.name = name;
		this.item = item;
		this.qty = qty;
	}

}
